{
    "name": "circuitid/sdk",
    "description": "A PHP wrapper for Circuit ID's REST API",
    "version": "1.0.0",
    "type": "library",
    "keywords": [ 
        "php",
        "sdk",
        "rest",
        "api",
        "cloud communications",
        "AI-powered",
        "calling",
        "meetings",
        "messaging",
        "voicemail",
        "fax",
        "SIP Trunking",
        "mobile broadband",
        "mobile phone services"
    ],
    "homepage": "https://github.com/circuitid/circuitid-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Circuit ID",
            "homepage": "http://www.circuitid.com/"
        }
    ],
    "support": {
        "source": "https://github.com/circuitid/circuitid-php",
        "docs": "https://rest.circuitid.com/doc.html"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    }
}
